function _init()
    defaults()
    game_splash,game_loop = 0,1
    game=game_splash
    show_splash()
    linux=create_bunny("linux",50,70,false,0)
    kitty=create_bunny("kitty",50,70,true,1)
    flowers={}
    for i=0,20 do
        add(flowers,create_flower())
    end
end

